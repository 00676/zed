  "border": {
    "base": {
      "width": 1
    },
    "primary": {
      "extends": "{border.base}",
      "color": "{border_color.primary}"
    },
    "secondary": {
      "extends": "{border.base}",
      "color": "{border_color.secondary}"
    },
    "muted": {
      "extends": "{border.base}",
      "color": "{border_color.muted}"
    },
    "minimal": {
      "extends": "{border.base}",
      "color": "{border_color.minimal}"
