    "secondary": {
      "extends": "{icon.base}",
      "color": "{icon_color.secondary}"
    },
    "muted": {
      "extends": "{icon.base}",
      "color": "{icon_color.muted}"
    },
    "active": {
      "extends": "{icon.base}",
      "color": "{icon_color.active}"
    },
    "disabled": {
      "extends": "{icon.base}",
      "color": "{icon_color.disabled}"
    },
    "positive": {
      "extends": "{icon.base}",
