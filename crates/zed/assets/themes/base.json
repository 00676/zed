{
  "chat_panel": {
    "extends": "$panel",
    "channel_name": {
      "extends": "$text.0",
      "weight": "bold"
    },
    "channel_name_hash": {
      "text": "$text.2",
      "padding": {
        "right": 8
      }
    },
    "channel_select": {
      "active_item": {
        "extends": "$chat_panel.channel_select.item",
        "name": "$text.0"
      },
      "header": {
        "extends": "$chat_panel.channel_select.active_item",
        "padding": {
          "bottom": 4,
          "left": 0
        }
      },
      "hovered_active_item": {
        "extends": "$chat_panel.channel_select.hovered_item",
        "name": "$text.0"
      },
      "hovered_item": {
        "background": "$state.hover",
        "corner_radius": 6,
        "extends": "$chat_panel.channel_select.item"
      },
      "item": {
        "name": "$text.1",
        "padding": 4,
        "hash": {
          "extends": "$text.2",
          "margin": {
            "right": 8
          }
        }
      },
      "menu": {
        "background": "$surface.0",
        "corner_radius": 6,
        "padding": 4,
        "border": {
          "color": "$border.0",
          "width": 1
        },
        "shadow": {
          "blur": 16,
          "color": "$shadow.0",
          "offset": [
            0,
            2
          ]
        }
      }
    },
    "hovered_sign_in_prompt": {
      "color": "$text.1.color",
      "extends": "$chat_panel.sign_in_prompt"
    },
    "input_editor": {
      "background": "$surface.1",
      "corner_radius": 6,
      "placeholder_text": "$text.2",
      "selection": "$selection.host",
      "text": "$text.0",
      "border": {
        "color": "$border.0",
        "width": 1
      },
      "padding": {
        "bottom": 7,
        "left": 8,
        "right": 8,
        "top": 7
      }
    },
    "message": {
      "body": "$text.1",
      "timestamp": "$text.2",
      "padding": {
        "bottom": 6
      },
      "sender": {
        "extends": "$text.0",
        "weight": "bold",
        "margin": {
          "right": 8
        }
      }
    },
    "pending_message": {
      "extends": "$chat_panel.message",
      "body": {
        "color": "$text.3.color"
      },
      "sender": {
        "color": "$text.3.color"
      },
      "timestamp": {
        "color": "$text.3.color"
      }
    },
    "sign_in_prompt": {
      "extends": "$text.0",
      "underline": true
    }
  },
  "contacts_panel": {
    "extends": "$panel",
    "host_row_height": 28,
    "tree_branch_color": "$surface.2",
    "tree_branch_width": 1,
    "host_avatar": {
      "corner_radius": 10,
      "width": 18
    },
    "host_username": {
      "extends": "$text.0",
      "padding": {
        "left": 8
      }
    },
    "hovered_shared_project": {
      "background": "$state.hover",
      "corner_radius": 6,
      "extends": "$contacts_panel.shared_project"
    },
    "hovered_unshared_project": {
      "background": "$state.hover",
      "corner_radius": 6,
      "extends": "$contacts_panel.unshared_project"
    },
    "project": {
      "guest_avatar_spacing": 4,
      "height": 24,
      "guest_avatar": {
        "corner_radius": 8,
        "width": 14
      },
      "name": {
        "extends": "$text.1",
        "margin": {
          "right": 6
        }
      },
      "padding": {
        "left": 8
      }
    },
    "shared_project": {
      "extends": "$contacts_panel.project",
      "name": {
        "color": "$text.0.color"
      }
    },
    "unshared_project": {
      "extends": "$contacts_panel.project"
    }
  },
  "editor": {
    "active_line_background": "$state.active_line",
    "background": "$surface.1",
    "code_actions_indicator": "$text.3.color",
    "diff_background_deleted": "$state.deleted_line",
    "diff_background_inserted": "$state.inserted_line",
    "document_highlight_read_background": "#99999920",
    "document_highlight_write_background": "#99999916",
    "error_color": "$status.bad",
    "guest_selections": "$selection.guests",
    "gutter_background": "$surface.1",
    "gutter_padding_factor": 2.5,
    "highlighted_line_background": "$state.highlighted_line",
    "line_number": "$text.2.color",
    "line_number_active": "$text.0.color",
    "selection": "$selection.host",
    "text_color": "$text.1.color",
    "autocomplete": {
      "background": "$surface.2",
      "corner_radius": 6,
      "padding": 6,
      "border": {
        "color": "$border.1",
        "width": 2
      },
      "hovered_item": {
        "background": "$state.hover",
        "extends": "$editor.autocomplete.item"
      },
      "item": {
        "corner_radius": 6,
        "padding": {
          "bottom": 2,
          "left": 6,
          "right": 6,
          "top": 2
        }
      },
      "margin": {
        "left": -14
      },
      "match_highlight": {
        "color": "$editor.syntax.keyword.color",
        "weight": "$editor.syntax.keyword.weight"
      },
      "selected_item": {
        "background": "$state.selected",
        "extends": "$editor.autocomplete.item"
      }
    },
    "diagnostic_header": {
      "background": "$editor.background",
      "icon_width_factor": 1.5,
      "text_scale_factor": 0.857,
      "border": {
        "bottom": true,
        "color": "$border.1",
        "top": true,
        "width": 1
      },
      "code": {
        "extends": "$text.2",
        "size": 14,
        "margin": {
          "left": 10
        }
      },
      "message": {
        "highlight_text": {
          "extends": "$text.0",
          "size": 14,
          "weight": "bold"
        },
        "text": {
          "extends": "$text.1",
          "size": 14
        }
      }
    },
    "diagnostic_path_header": {
      "background": "$state.active_line",
      "text_scale_factor": 0.857,
      "filename": {
        "extends": "$text.0",
        "size": 14
      },
      "path": {
        "extends": "$text.2",
        "size": 14,
        "margin": {
          "left": 12
        }
      }
    },
    "error_diagnostic": {
      "text_scale_factor": 0.857,
      "header": {
        "border": {
          "color": "$border.0",
          "top": true,
          "width": 1
        }
      },
      "message": {
        "highlight_text": {
          "color": "$status.bad",
          "extends": "$text.1",
          "size": 14,
          "weight": "bold"
        },
        "text": {
          "color": "$status.bad",
          "extends": "$text.1",
          "size": 14
        }
      }
    },
    "hint_diagnostic": {
      "extends": "$editor.error_diagnostic",
      "message": {
        "highlight_text": {
          "color": "$status.info"
        },
        "text": {
          "color": "$status.info"
        }
      }
    },
    "information_diagnostic": {
      "extends": "$editor.error_diagnostic",
      "message": {
        "highlight_text": {
          "color": "$status.info"
        },
        "text": {
          "color": "$status.info"
        }
      }
    },
    "invalid_error_diagnostic": {
      "extends": "$editor.error_diagnostic",
      "message": {
        "highlight_text": {
          "color": "$text.3.color"
        },
        "text": {
          "color": "$text.3.color"
        }
      }
    },
    "invalid_hint_diagnostic": {
      "extends": "$editor.hint_diagnostic",
      "message": {
        "highlight_text": {
          "color": "$text.3.color"
        },
        "text": {
          "color": "$text.3.color"
        }
      }
    },
    "invalid_information_diagnostic": {
      "extends": "$editor.information_diagnostic",
      "message": {
        "highlight_text": {
          "color": "$text.3.color"
        },
        "text": {
          "color": "$text.3.color"
        }
      }
    },
    "invalid_warning_diagnostic": {
      "extends": "$editor.warning_diagnostic",
      "message": {
        "highlight_text": {
          "color": "$text.3.color"
        },
        "text": {
          "color": "$text.3.color"
        }
      }
    },
    "warning_diagnostic": {
      "extends": "$editor.error_diagnostic",
      "message": {
        "highlight_text": {
          "color": "$status.warn"
        },
        "text": {
          "color": "$status.warn"
        }
      }
    }
  },
  "panel": {
    "padding": {
      "bottom": 12,
      "left": 12,
      "right": 12,
      "top": 12
    }
  },
  "project_diagnostics": {
    "background": "$surface.1",
    "tab_icon_spacing": 4,
    "tab_icon_width": 13,
    "tab_summary_spacing": 10,
    "empty_message": {
      "extends": "$text.0",
      "size": 18
    },
    "status_bar_item": {
      "extends": "$text.2",
      "margin": {
        "right": 10
      }
    }
  },
  "project_panel": {
    "extends": "$panel",
    "entry": {
      "height": 22,
      "icon_color": "$text.3.color",
      "icon_size": 8,
      "icon_spacing": 8,
      "text": "$text.1"
    },
    "hovered_entry": {
      "background": "$state.hover",
      "extends": "$project_panel.entry"
    },
    "hovered_selected_entry": {
      "extends": "$project_panel.hovered_entry",
      "text": {
        "extends": "$text.0"
      }
    },
    "padding": {
      "top": 6
    },
    "selected_entry": {
      "extends": "$project_panel.entry",
      "text": {
        "extends": "$text.0"
      }
    }
  },
  "search": {
    "background": "$surface.1",
    "match_background": "$state.highlighted_line",
    "tab_icon_spacing": 4,
    "tab_icon_width": 14,
    "active_hovered_option_button": {
      "background": "$surface.2",
      "extends": "$search.option_button"
    },
    "active_option_button": {
      "background": "$surface.2",
      "extends": "$search.option_button"
    },
    "editor": {
      "background": "$surface.0",
      "corner_radius": 6,
      "max_width": 400,
      "placeholder_text": "$text.2",
      "selection": "$selection.host",
      "text": "$text.0",
      "border": {
        "color": "$border.0",
        "width": 1
      },
      "margin": {
        "bottom": 5,
        "left": 5,
        "right": 5,
        "top": 5
      },
      "padding": {
        "bottom": 3,
        "left": 13,
        "right": 13,
        "top": 3
      }
    },
    "hovered_option_button": {
      "background": "$surface.2",
      "extends": "$search.option_button"
    },
    "invalid_editor": {
      "extends": "$search.editor",
      "border": {
        "color": "$status.bad",
        "width": 1
      }
    },
    "match_index": {
      "extends": "$text.1",
      "padding": 6
    },
    "option_button": {
      "background": "$surface.1",
      "corner_radius": 6,
      "extends": "$text.1",
      "border": {
        "color": "$border.0",
        "width": 1
      },
      "margin": {
        "left": 1,
        "right": 1
      },
      "padding": {
        "bottom": 1,
        "left": 6,
        "right": 6,
        "top": 1
      }
    },
    "option_button_group": {
      "padding": {
        "left": 2,
        "right": 2
      }
    },
    "results_status": {
      "extends": "$text.0",
      "size": 18
    }
  },
  "selector": {
    "background": "$surface.0",
    "corner_radius": 6,
    "padding": 8,
    "active_item": {
      "background": "$state.hover",
      "extends": "$selector.item",
      "text": "$text.0"
    },
    "border": {
      "color": "$border.0",
      "width": 1
    },
    "empty": {
      "text": "$text.2",
      "padding": {
        "bottom": 4,
        "left": 16,
        "right": 16,
        "top": 8
      }
    },
    "input_editor": {
      "background": "$surface.1",
      "corner_radius": 6,
      "placeholder_text": "$text.2",
      "selection": "$selection.host",
      "text": "$text.0",
      "border": {
        "color": "$border.0",
        "width": 1
      },
      "padding": {
        "bottom": 7,
        "left": 16,
        "right": 16,
        "top": 7
      }
    },
    "item": {
      "corner_radius": 6,
      "text": "$text.1",
      "highlight_text": {
        "color": "$editor.syntax.keyword.color",
        "extends": "$text.base",
        "weight": "$editor.syntax.keyword.weight"
      },
      "padding": {
        "bottom": 4,
        "left": 16,
        "right": 16,
        "top": 4
      }
    },
    "margin": {
      "bottom": 52,
      "top": 52
    },
    "shadow": {
      "blur": 16,
      "color": "$shadow.0",
      "offset": [
        0,
        2
      ]
    }
  },
  "text": {
    "base": {
      "family": "Zed Sans",
      "size": 14
    }
  },
  "workspace": {
    "background": "$surface.0",
    "active_tab": {
      "background": "$surface.1",
      "extends": "$workspace.tab",
      "text": "$text.0",
      "border": {
        "bottom": false
      }
    },
    "left_sidebar": {
      "extends": "$workspace.sidebar",
      "border": {
        "color": "$border.0",
        "right": true,
        "width": 1
      }
    },
    "pane_divider": {
      "color": "$border.0",
      "width": 1
    },
    "right_sidebar": {
      "extends": "$workspace.sidebar",
      "border": {
        "color": "$border.0",
        "left": true,
        "width": 1
      }
    },
    "sidebar": {
      "width": 30,
      "active_item": {
        "extends": "$workspace.sidebar.item",
        "icon_color": "$text.0.color"
      },
      "border": {
        "color": "$border.0",
        "right": true,
        "width": 1
      },
      "item": {
        "height": "$workspace.tab.height",
        "icon_color": "$text.2.color",
        "icon_size": 18
      },
      "resize_handle": {
        "background": "$border.0",
        "padding": {
          "left": 1
        }
      }
    },
    "status_bar": {
      "cursor_position": "$text.2",
      "diagnostic_message": "$text.2",
      "height": 24,
      "item_spacing": 8,
      "lsp_message": "$text.2",
      "padding": {
        "left": 6,
        "right": 6
      }
    },
    "tab": {
      "height": 34,
      "icon_close": "$text.2.color",
      "icon_close_active": "$text.0.color",
      "icon_conflict": "$status.warn",
      "icon_dirty": "$status.info",
      "icon_width": 8,
      "spacing": 10,
      "text": "$text.2",
      "border": {
        "bottom": true,
        "color": "$border.0",
        "left": true,
        "overlay": true,
        "width": 1
      },
      "padding": {
        "left": 12,
        "right": 12
      }
    },
    "titlebar": {
      "avatar_width": 18,
      "height": 32,
      "share_icon_active_color": "$text.0.color",
      "share_icon_color": "$text.2.color",
      "title": "$text.0",
      "avatar": {
        "corner_radius": 10,
        "border": {
          "color": "#00000088",
          "width": 1
        }
      },
      "avatar_ribbon": {
        "background": "#ff0000",
        "height": 3,
        "width": 12
      },
      "border": {
        "bottom": true,
        "color": "$border.0",
        "width": 1
      },
      "hovered_sign_in_prompt": {
        "color": "$text.1.color",
        "extends": "$workspace.titlebar.sign_in_prompt"
      },
      "offline_icon": {
        "color": "$text.2.color",
        "width": 16,
        "padding": {
          "right": 4
        }
      },
      "outdated_warning": {
        "extends": "$text.2",
        "size": 13
      },
      "sign_in_prompt": {
        "extends": "$text.2",
        "size": 13,
        "underline": true,
        "padding": {
          "right": 8
        }
      }
    },
    "toolbar": {
      "height": 44
    }
  }
}