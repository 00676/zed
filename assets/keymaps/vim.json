{
    "Editor && VimControl": {
        "i": [
            "vim::SwitchMode",
            "Insert"
        ],
        "g": [
            "vim::PushOperator",
            {
                "Namespace": "G"
            }
        ],
        "h": "vim::Left",
        "j": "vim::Down",
        "k": "vim::Up",
        "l": "vim::Right",
        "0": "vim::StartOfLine",
        "shift-$": "vim::EndOfLine",
        "shift-G": "vim::EndOfDocument",
        "w": "vim::NextWordStart",
        "shift-W": [
            "vim::NextWordStart",
            {
                "ignorePunctuation": true
            }
        ],
        "e": "vim::NextWordEnd",
        "shift-E": [
            "vim::NextWordEnd",
            {
                "ignorePunctuation": true
            }
        ],
        "b": "vim::PreviousWordStart",
        "shift-B": [
            "vim::PreviousWordStart",
            {
                "ignorePunctuation": true
            }
        ],
        "escape": [
            "vim::SwitchMode",
            "Normal"
        ]
    },
    "Editor && vim_operator == g": {
        "g": "vim::StartOfDocument"
    },
    "Editor && vim_mode == insert": {
        "escape": "vim::NormalBefore",
        "ctrl-c": "vim::NormalBefore"
    },
    "Editor && vim_mode == normal": {
        "c": [
            "vim::PushOperator",
            "Change"
        ],
        "d": [
            "vim::PushOperator",
            "Delete"
        ]
    },
    "Editor && vim_operator == c": {
        "w": [
            "vim::NextWordEnd",
            {
                "ignorePunctuation": false
            }
        ],
        "shift-W": [
            "vim::NextWordEnd",
            {
                "ignorePunctuation": true
            }
        ]
    },
    "Editor && vim_operator == d": {
        "w": [
            "vim::NextWordStart",
            {
                "ignorePunctuation": false,
                "stopAtNewline": true
            }
        ],
        "shift-W": [
            "vim::NextWordStart",
            {
                "ignorePunctuation": true,
                "stopAtNewline": true
            }
        ]
    }
}